{
    "name": "7981620/laravel10-loadmore",
    "description": "Allow pagination to load an initial number of items on the first page and subsequent pages can load a different number of items",
    "keywords": [],
    "homepage": "https://github.com/7981620/laravel10-loadmore",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Sabil",
            "email": "me@mohamedsabil83.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Agenta\\LaravelLoadmore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Agenta\\LaravelLoadmore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
