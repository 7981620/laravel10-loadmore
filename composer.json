{
    "name": "mohamedsabil83/laravel-loadmore",
    "description": "Allow pagination to load an initial number of items on the first page and subsequent pages can load a different number of items",
    "keywords": [
        "mohamedsabil83",
        "laravel",
        "loadmore",
        "pagination"
    ],
    "homepage": "https://github.com/mohamedsabil83/laravel-loadmore",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Sabil",
            "email": "me@mohamedsabil83.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/support": "5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mohamedsabil83\\LaravelLoadmore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mohamedsabil83\\LaravelLoadmore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
